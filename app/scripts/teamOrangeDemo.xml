<application>
<name>Team Orange</name>

<dependencies>
    <port>/icub/camcalib/left/out</port>
    <port>/lbpExtract/extractedlbp:o</port>

</dependencies>

<!-- Modules configurations -->

<module>
    <name>master</name>
    <parameters>--whostarts robot</parameters>
    <parameters>--initBoardPosition "(0 1 2 0 1 2)"</parameters>
    <parameters>--GoalInit "(1 1 1)"</parameters>
    <node>localhost</node>
</module>

<module>
    <name>objReco</name>
    <parameters>--parameter1</parameters>
    <node>localhost</node>
</module>

<module>
    <name>pickAndPlace</name>
    <node>localhost</node>
</module>

<module>
    <name>clapDuo</name>
    <node>localhost</node>
</module>

<module>
    <name>moveGrounding</name>
    <node>localhost</node>
</module>

<module>
    <name>movePlanner</name>
    <node>localhost</node>
</module>

<!-- Connections of Modules -->

<!-- rpcClient connection to Object Recognition Module-->
<connection>
    <from>/objReco/rpc:o</from>
    <to>/master/rpc:i</to>
</connection>

<!-- rpcClient connection to ClapDuo Module-->
<connection>
    <from>/clapDuo/rpc:o</from>
    <to>/master/rpc:i</to>
</connection>

<!-- rpcClient connection to Move Planner Module-->
<connection>
    <from>/movePlanner/rpc:o</from>
    <to>/moveGrouding/rpc:i</to>
</connection>

<!-- rpcClient connection to Pick And Place Module-->
<connection>
    <from>/pickAndPlace/rpc:o</from>
    <to>/master/rpc:i</to>
</connection>

<!-- rpcClient connection to Move Grounding Module-->
<connection>
    <from>/moveGrounding/rpc:o</from>
    <to>/master/rpc:i</to>
</connection>

</application>
